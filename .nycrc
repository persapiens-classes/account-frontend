{
  "report-dir": "coverage/e2e",
  "reporter": ["html", "lcov", "text", "text-summary", "json"],
  "exclude": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/*.e2e-spec.ts",
    "**/test-setup.ts",
    "**/environments/**",
    "**/*.config.ts",
    "**/*.config.js",
    "**/cypress/**",
    "**/coverage/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/src/app/shared/test-utils.ts"
  ],
  "temp-dir": ".nyc_output",
  "all": true,
  "include": ["src/**/*.ts"],
  "extension": [".ts"],
  "sourceMap": true,
  "instrument": true
}
